</span><span class='line'>    scanf("%d %d",&amp;n,&amp;m);
</span><span class='line'>    if(0==n)break;
</span><span class='line'>    for(i=0;i&lt;100;i++)
</span><span class='line'>        for(j=0;j&lt;100;j++)
</span><span class='line'>            cost[i][j]=INF;
</span><span class='line'>    for(i=0;i&lt;n;i++){
</span><span class='line'>        scanf("%d %d %d",&amp;a,&amp;b,&amp;c);
</span><span class='line'>        cost[a-1][b-1]=cost[b-1][a-1]=c;
</span><span class='line'>    }
</span><span class='line'>    min=solve();
</span><span class='line'>    if(min==-1)printf("?\n");
</span><span class='line'>    else printf("%d\n",min);
